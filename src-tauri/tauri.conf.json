{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "alinavi",
  "version": "0.1.0",
  "identifier": "com.alinavi.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "alinavi",
        "minHeight": 250,
        "minWidth": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@x2.png",
      "icons/icon.ico"
    ]
  }
}
